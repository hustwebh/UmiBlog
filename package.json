{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "classnames": "^2.3.1",
    "crypto-js": "^4.1.1",
    "dva": "^2.4.1",
    "markdown-it": "^13.0.1",
    "react": "17.x",
    "react-dom": "17.x",
    "react-mathjax": "^1.0.1",
    "react-syntax-highlighter": "^15.5.0",
    "umi": "^4.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-mathjax": "^1.0.1",
    "@types/three": "^0.141.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.26",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
